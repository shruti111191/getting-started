		System.out.println("Hello World");
	}
}


import java.util.*;
public class Main
{
	public static void main(String[] args) {
		System.out.println("*");
		System.out.println("**");
		System.out.println("***");
		System.out.println("****");
		System.out.println("*****");
	}
}

